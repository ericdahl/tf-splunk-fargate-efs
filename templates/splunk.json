[
  {
    "name": "splunk",
    "image": "669361545709.dkr.ecr.us-east-1.amazonaws.com/docker-efs:latest",
    "portMappings": [
      {
        "containerPort": 8000,
        "hostPort": 8000,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "SPLUNK_START_ARGS",
        "value": "--accept-license"
      },
      {
        "name": "SPLUNK_PASSWORD",
        "value": "password"
      }
    ]
  }
]
